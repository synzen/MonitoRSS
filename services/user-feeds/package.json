{
  "name": "monitorss-user-feeds-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=local nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sync-db": "mikro-orm schema:update --run",
    "migration:local:create": "cross-env NODE_ENV=local mikro-orm migration:create",
    "migration:local:up": "cross-env NODE_ENV=local mikro-orm migration:up",
    "migration:local:list": "cross-env NODE_ENV=local mikro-orm migration:list",
    "migration:local:pending": "cross-env NODE_ENV=local mikro-orm migration:pending",
    "migration:local:down": "cross-env NODE_ENV=local mikro-orm migration:down",
    "migration:prod:create": "cross-env NODE_ENV=production mikro-orm migration:create",
    "migration:prod:up": "cross-env NODE_ENV=production mikro-orm migration:up"
  },
  "dependencies": {
    "@fastify/compress": "^6.4.0",
    "@golevelup/nestjs-rabbitmq": "^4.0.0",
    "@grpc/grpc-js": "^1.9.11",
    "@grpc/proto-loader": "^0.7.8",
    "@mikro-orm/core": "^5.7.12",
    "@mikro-orm/nestjs": "^5.2.0",
    "@mikro-orm/postgresql": "^5.7.12",
    "@monitorss/logger": "^1.1.2",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.1.3",
    "@nestjs/platform-fastify": "^10.1.3",
    "@synzen/discord-rest": "^0.7.0-beta.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "feedparser": "^2.2.10",
    "flat": "^5.0.2",
    "html-to-text": "^9.0.3",
    "node-html-parser": "^6.1.5",
    "object-path": "^0.11.8",
    "p-retry": "^4.6.2",
    "qs": "^6.11.0",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "undici": "^5.22.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.7.12",
    "@mikro-orm/migrations": "^5.7.12",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1.3",
    "@types/feedparser": "^2.2.5",
    "@types/flat": "^5.0.2",
    "@types/html-to-text": "^9.0.0",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/object-path": "^0.11.1",
    "@types/qs": "^6.9.7",
    "@types/rascal": "^10.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
