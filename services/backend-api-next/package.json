{
  "name": "backend-api-next",
  "main": "dist/index.js",
  "private": true,
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.2",
    "@fastify/secure-session": "^8.1.1",
    "@fastify/static": "^8.1.0",
    "@monitorss/logger": "^1.1.2",
    "@sinclair/typebox": "^0.34.48",
    "@discordjs/rest": "^2.6.0",
    "commander": "^12.0.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "feedparser": "^2.2.10",
    "handlebars": "^4.7.8",
    "html-to-text": "^8.2.1",
    "moment-timezone": "^0.5.46",
    "mongoose": "^7.6.2",
    "nodemailer": "^6.9.16",
    "qs": "^6.14.1",
    "rabbitmq-client": "^5.0.8",
    "undici": "^7.16.0",
    "zod": "^3.23.8"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
    "start": "node ./dist/index.js",
    "start:schedule-emitter": "node ./dist/schedule-emitter.js",
    "dev": "tsx watch index.ts",
    "dev:schedule-emitter": "tsx watch schedule-emitter.ts",
    "test": "node --import tsx --test --test-concurrency=1 \"test/**/*.test.ts\"",
    "test:docker": "bash test-e2e.sh",
    "enforce-limits": "tsx src/scripts/enforce-limits.ts",
    "migrate-legacy-feeds": "tsx src/scripts/migrate-all-legacy-feeds.ts",
    "recalculate-slots": "tsx src/scripts/recalculate-slot-offsets.ts",
    "e2e": "npx playwright test",
    "e2e:ui": "npx playwright test --ui",
    "e2e:auth": "npx playwright codegen http://localhost:3000 --save-storage=e2e/auth.json",
    "e2e:report": "npx playwright show-report"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/feedparser": "^2.2.8",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/qs": "^6.14.0",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
